{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": [
      "./src/types"
    ],
    "noImplicitReturns": false,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "assets/*": [
        "assets/*"
      ],
      "screens/*": [
        "src/modules/auth/screens/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "resolveJsonModule": true,
    "noImplicitAny": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "node_modules",
    "babel.config.js",
    "index.js"
  ]
}
